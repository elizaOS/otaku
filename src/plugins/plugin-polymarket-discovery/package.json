{
  "name": "@elizaos/plugin-polymarket-discovery",
  "version": "1.6.2-alpha.14",
  "description": "Polymarket prediction markets discovery plugin for ElizaOS (read-only market data and analysis)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun run build.ts --watch",
    "typecheck": "tsc --project tsconfig.build.json --noEmit"
  },
  "dependencies": {
    "@elizaos/core": "alpha",
    "@polymarket/sdk": "^6.0.1"
  },
  "peerDependencies": {
    "@elizaos/core": "alpha"
  },
  "devDependencies": {
    "@types/bun": "^1.2.21",
    "@types/node": "^22.15.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.2.0"
  },
  "publishConfig": {
    "access": "public",
    "workspaces": {
      "preserveIfPresent": true
    }
  },
  "keywords": [
    "polymarket",
    "prediction-markets",
    "betting",
    "defi",
    "eliza",
    "elizaos",
    "plugin"
  ],
  "author": "ElizaOS",
  "license": "MIT"
}
